# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.js]
charset = utf-8
indent_style = space
indent_size = 2

[*.svelte]
charset = utf-8
indent_style = space
indent_size = 2

[*.html]
charset = utf-8
indent_style = space
indent_size = 2
